{
  "$schema": "http://json.schemastore.org/template",
  "author": "Sodiq Olalekan Yekeen",
  "classifications": ["Web", "Api", "Blazor"],
  "identity": "NetApp.Template",
  "name": "My Custom .Net App Template",
  "sourceName": "NetApp",
  "shortName": "netapp",
  "tags": {
    "language": "C#",
    "type": "project"
  },
  "preferNameDirectory": true,
  "description": "Custom Fullstack .Net App Template",
  "symbols": {
    "Framework": {
      "type": "parameter",
      "description": "The traget framework for the Application",
      "datatype": "choice",
      "defaultValue": "net7.0",
      "choices": [
        {
          "choice": "net6.0"
        },
        {
          "choice": "net7.0"
        }
      ]
    }
  },
  "sources": [
    {
      "exclude": [
        "**/[Bb]in/**",
        "**/[Oo]bj/**",
        ".template.config/**/*",
        "**/*.filelist",
        "**/*.user",
        "**/*.lock.json"
      ]
    }
  ]
}
